{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Information"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "data source=/data/token.db",
        "LoggerConnection": "data source=/data/logger.db",
        "DBType": "sqlite"
        // sqlite | [postgresql,pgsql] | [sqlserver,mssql] | mysql
    },
    "Chat": {
        "Master": "",
        "Shared": {
            "EnableShareAd": true,
            "ShareCredit": 5000,
            "ShareLimit": 5
        },
        "FreeModel": {
            "EnableFree": false,
            "Items": [
                {
                    "Model": [ "gpt-3.5-turbo-0125" ],
                    "Limit": 2
                }
            ]
        }
    },
    "Cache": {
        "Type": "Memory",
        "ConnectionString": ""
    },
    "Jwt": {
        "Secret": "asd?fgahjwter.223(%^klqwter.123(%^werqwter.123(%^$%#",
        "EffectiveHours": 120
    },
    "Github": {
        "Endpoint": "https://github.com",
        "APIEndpoint": "https://api.github.com"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Microsoft": "Information",
                "System": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs\\log-.txt",
                    "rollingInterval": "Day",
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ],
        "Properties": {
            "Application": "AI Gateway"
        }
    },
    "OTEL_EXPORTER_OTLP_ENDPOINT": "http://aspire-dashboard:18889"
}
